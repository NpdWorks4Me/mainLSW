{
  "name": "little-space-world",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prebuild": "node ./scripts/check-env.cjs",
    "generate-config": "node ./scripts/generate-config.cjs",
    "check-config": "node ./scripts/check-config.cjs",
    "vercel-build": "npm run generate-config && npm run check-config && echo 'vercel build unsupported: Next.js admin removed'",
    
  "build:main": "npm run build:client",
  "build:main-only": "npm run build:client",
  "dev:main": "npm run dev:client",
  "start:main": "npm run preview:client",
  "build:client": "vite build --config src/vite.config.js",
  "dev:client": "vite --config src/vite.config.js --host :: --port 3000",
  "preview:client": "vite preview --config src/vite.config.js --host :: --port 3000",
    "build": "npm run generate-config && npm run check-config && npm run build:main",
    "postbuild": "node ./scripts/prepare-root-dist.js && node ./scripts/write-vercel-json.js",
    "export": "echo 'next export unsupported: admin removed'",
    "dev": "npm run dev:main",
    "start": "npm run start:main",
    "verify:local": "./scripts/verify-local-assets.sh http://localhost:3000",
    "verify:deploy": "./scripts/verify-hosting-deploy.sh",
    "deploy:hostinger": "npm run build && node ./scripts/redeploy-main.js && bash ./scripts/verify-public-html-assets.sh",
    "verify:console": "node ./scripts/playwright-check-store.js http://localhost:3000/store/",
  "preview": "npm run preview:client",
    "preview:public": "npx serve -s public_html -l 3000",
    "preview:dist": "npx serve -s dist -l 3000",
    "test:smoke": "PLAYWRIGHT_SKIP_ADMIN_WEB_SERVER=true npx playwright test tests/homepage.spec.js -g \"Homepage smoke tests\"",
    "test:smoke:store": "PLAYWRIGHT_SKIP_ADMIN_WEB_SERVER=true npx playwright test tests/store.spec.js -g \"Store smoke tests\"",
    "test:smoke:all": "npm run test:smoke && npm run test:smoke:store",
    "check:predeploy": "node ./scripts/check-deploy-type.js --dir dist",
    "check:dist-no-admin": "node ./scripts/check-dist-no-admin.js --dir dist",
    "sitemap": "node scripts/generate-site-map.js && node scripts/refine-site-map.js && node scripts/generate-xml-sitemap.mjs",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-label": "^1.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.1.0",
    "@tanstack/react-table": "^8.8.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^1.2.1",
  "chart.js": "^4.4.3",
  "date-fns": "^3.0.6",
  "embla-carousel-autoplay": "^8.0.0-rc19",
  "embla-carousel-react": "^8.0.4",
  "gsap": "^3.12.5",
  "react-chartjs-2": "^5.2.0",
  "react-icons": "^5.4.0",
  "react-quill": "^2.0.0",
  "react-router-dom": "^6.16.0",
  "swiper": "^11.1.4",
  "uuid": "^9.0.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "nipplejs": "^0.9.0",
    "node-fetch": "^3.3.2",
    "phaser": "^3.60.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
  "react-helmet-async": "^2.0.0",
    "source-map": "^0.7.4",
    "stripe": "^12.10.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.17",
    "autoprefixer": "^10.4.22",
  "@vitejs/plugin-react": "^4.0.3",
  "vite": "^4.4.5",
    "eslint": "^8.57.0",
    "fs-extra": "^11.1.1",
    "madge": "^8.0.0",
    "postcss-import": "^16.1.1",
    "prettier": "^3.3.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.0"
  }
}
